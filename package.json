{
  "name": "spicy-sea-shells-site",
  "private": true,
  "version": "0.1.0",
  "description": "A blog for the Spicy Sea Shells",
  "author": "Sophie Au <sophie@sophieau.com>",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "prod": "gatsby build && gatsby serve",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --write '{src,data}/**/*.{tsx,ts}'",
    "clean": "gatsby clean",
    "checks": "tsc && npm run lint",
    "lint": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts' --ignore-pattern 'src/assets/*' && stylelint 'src/**/*.styles.ts' --rd --risd",
    "stylelint": "stylelint '.' --rd --risd",
    "lint:fix": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts'  --ignore-pattern 'src/assets/*' --fix && stylelint 'src/**/*.styles.ts' --rd --risd --fix"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "fs-extra": "^9.0.0",
    "gatsby": "^2.21.7",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-feed": "^2.5.0",
    "gatsby-plugin-linaria": "^2.0.0",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-sitemap": "^2.4.0",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-remark-images": "^3.3.0",
    "gatsby-remark-prismjs": "^3.5.2",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-remark": "^2.8.3",
    "gatsby-transformer-sharp": "^2.5.0",
    "linaria": "^2.0.0-alpha.2",
    "prismjs": "^1.20.0",
    "puppeteer": "^3.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "2.0.5",
    "stylelint": "^13.3.3",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SophieAu/spicy-sea-shells-site"
  }
}
