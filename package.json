    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.15.3",
    "typescript": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SophieAu/spicy-sea-shells-site"
