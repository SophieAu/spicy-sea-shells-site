{
  "name": "spicy-sea-shells-site",
  "private": true,
  "version": "0.1.0",
  "description": "A blog for the Spicy Sea Shells",
  "author": "Sophie Au <sophie@sophieau.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prod": "gatsby build && gatsby serve",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --write '{src,data}/**/*.{tsx,ts}'",
    "clean": "gatsby clean",
    "checks": "tsc && npm run lint",
    "lint": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts' --ignore-pattern 'src/assets/*' && stylelint 'src/**/*.{css,scss,styles.ts}' --rd --risd",
    "stylelint": "stylelint '.' --rd --risd",
    "lint:fix": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts'  --ignore-pattern 'src/assets/*' --fix && stylelint 'src/**/*.{css,scss,styles.ts}' --rd --risd --fix"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "fs-extra": "^9.0.0",
    "gatsby": "^2.20.9",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-feed": "^2.4.1",
    "gatsby-plugin-linaria": "^2.0.0",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "linaria": "^1.4.0-beta.5",
    "node-sass": "^4.13.1",
    "prismjs": "^1.19.0",
    "puppeteer": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "prettier": "2.0.2",
    "stylelint": "^13.2.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SophieAu/spicy-sea-shells-site"
  }
}
