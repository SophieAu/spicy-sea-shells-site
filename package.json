{
  "name": "spicy-sea-shells-site",
  "private": true,
  "version": "0.1.0",
  "description": "A blog for the Spicy Sea Shells",
  "author": "Sophie Au <sophie@sophieau.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prod": "gatsby build && gatsby serve",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --write '{src,data}/**/*.{tsx,ts}'",
    "clean": "gatsby clean",
    "checks": "tsc && npm run lint",
    "lint": "eslint '{src,data}/**/*.{ts,tsx}' --ignore-pattern '*.d.ts'",
    "lint:fix": "eslint '{src,data}/**/*.{ts,tsx}' --ignore-pattern '*.d.ts' --fix"
  },
  "dependencies": {
    "commentbox.io": "^2.0.0",
    "gatsby": "^2.18.21",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-feed": "^2.3.26",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.31",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.24",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-remark-prismjs": "^3.3.29",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-remark": "^2.6.39",
    "gatsby-transformer-sharp": "^2.3.7",
    "node-sass": "^4.13.0",
    "prismjs": "^1.18.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "prettier": "1.19.1",
    "typescript": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SophieAu/spicy-sea-shells-site"
  }
}
