{
  "name": "spicy-sea-shells-site",
  "private": true,
  "version": "0.1.0",
  "description": "A blog for the Spicy Sea Shells",
  "author": "Sophie Au <sophie@sophieau.com>",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "prod": "gatsby build && gatsby serve",
    "format": "prettier --write '{src,data}/**/*.{tsx,ts}'",
    "checks": "tsc && npm run lint",
    "lint": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts' --ignore-pattern 'src/assets/*' && stylelint 'src/**/*.styles.ts' --rd --risd",
    "stylelint": "stylelint '.' --rd --risd",
    "lint:fix": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts'  --ignore-pattern 'src/assets/*' --fix && stylelint 'src/**/*.styles.ts' --rd --risd --fix"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "core-js": "^3.6.5",
    "gatsby": "^2.23.12",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-feed-mdx": "^1.0.1",
    "gatsby-plugin-linaria": "^2.0.0",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-mdx": "^1.2.19",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-plugin-sitemap": "^2.4.7",
    "gatsby-plugin-typescript": "^2.4.8",
    "gatsby-remark-images": "^3.3.14",
    "gatsby-remark-prismjs": "^3.5.6",
    "gatsby-remark-social-image": "^1.1.0",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-transformer-sharp": "^2.5.7",
    "linaria": "^2.0.0-alpha.5",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^3.9.7"
  }
}
